{
    "name": "ahmed/laravel-todocrud",
    "description": "A simple to-do list package for Laravel to test package.",
    "type": "todolist",
    "keywords": [
        "laravel-todocrud",
        "laravel",
        "php"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ahmed\\Todolist\\": "src/"
        }
    },
    "authors": [
        {
            "name": "ahmedSohelcu",
            "email": "ahmedsohelcu@gmail.com"
        }
    ],
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "\\Ahmed\\Todocrud\\Providers\\TodoPackageServiceProvider"                        
            ]
        }
    }
	
    
}